<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Project_Android" parent="android:Theme.Material.Light.NoActionBar"/>


    <style name="Mythema" parent="android:Theme.Material.Light.NoActionBar" />
    <style name="WhiteStyle" ><item name="android:background">@color/white</item></style>
    <style name="BlueStyle" ><item name="android:background">#5197B6</item></style>
    <style name="TealStyle" ><item name="android:background">@color/teal_200</item></style>
    <style name="BlackStyle" ><item name="android:background">@color/black</item></style>
    <style name="PurpleStyleDark" ><item name="android:background">#4E0101</item></style>
    <style name="Red200Style" ><item name="android:background">#731616</item></style>
    <style name="PurpleStyleLight" ><item name="android:background">#BC0404</item></style>
    <style name="GreenText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#00FF00</item>
    </style>

    <style name="PurpleButton" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">#1a237e</item>
        <item name="colorControlHighlight">#673ab7</item>
    </style>

    <style name="RoundedButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/rounded_button</item>
    </style>

    <style name="RoundedButtStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/rounded_butt</item>
    </style>

    <style name="CustomMenuItemStyle">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#0F7E88</item>
    </style>

    <style name="PreferenceTheme" parent="PreferenceThemeOverlay">
        <item name="android:background">@color/lightmilk</item>
    </style>

</resources>